/* pages/date-list/index.wxss */
.date-wrap {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top: 20rpx;
  padding: 10rpx;
}
.date {
  box-sizing: border-box;
  width: 15vw;
  /* height: 130rpx; */
  /* padding: 10rpx 5rpx; */
  margin-bottom: 30rpx;
  box-shadow: 5rpx 5rpx 5rpx #fccfd1;

}
.desc li {
  display: block;
  font-size: 20rpx;
  font-weight: bold;
  margin-bottom: 10rpx;
  color: rgb(245, 170, 170);
}